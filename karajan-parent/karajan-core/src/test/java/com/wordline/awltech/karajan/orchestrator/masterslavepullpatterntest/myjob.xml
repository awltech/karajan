<job id="myjob" batchsize="5">
	<step id="step1" next="step2" ref="implementation1" parallelize="5">
		<errorhandling>
		 <exception on="ProcessorException" strategy="ALL" action="skippe" try="5" />
		 <exception on="RuntimeException" strategy="ONE" action="retry" try="1" />
		</errorhandling>
	</step>
	<step id="step2"  ref="implementation2" parallelize="5">
		<errorhandling>
		 <exception on="Exception" strategy="ONE" action="retry" try="1" />
		</errorhandling>
	</step>
</job>